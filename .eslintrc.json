{
    "env": {
        "es2021": true,
        "browser": true,
        "node": true,
        "jest":true
    },
    "extends": [
        "airbnb-base", "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/prefer-default-export": "off",
    "import/extensions": [
      "off"
    ],
    "no-underscore-dangle": "off"
    }
}
